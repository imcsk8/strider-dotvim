snippet table2
	<table id="tabl-${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}">
	  <title>${2}</title>
	  <tgroup cols="2">
	    <colspec colnum="1" colwidth="2*"></colspec>
	    <colspec colnum="2" colwidth="2*"></colspec>
	      <thead>
		      <row>
			      <entry>${3}</entry>
			      <entry>${4}</entry>
		      </row>
	      </thead>
	      <tbody>
		      <row>
			      <entry>${5}</entry>
			      <entry>${6}</entry>
		      </row>
		      <row>
			      <entry>${7}</entry>
			      <entry>${8}</entry>
		      </row>
	      </tbody>
	  </tgroup>
	</table>
